{
  "include": [
    "src/*",
    "src/component/Header/CommandLine",
    "declaration.d.ts"
],
  "exclude": [
    "node_modules",
    "build"
  ],
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es2020",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true
    }
}