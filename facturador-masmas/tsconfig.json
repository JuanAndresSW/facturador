{
  "include": ["src/*"],
  "exclude": [
    "node_modules",
    "build"
  ],
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "ES2020",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./src/",
      "paths": {
        "adapters/*":["./adapters/*"],
        "assets/*":["./assets/*"],
        "components/*":["./components/*"],
        "interceptors/*":["./interceptors/*"],
        "models/*":["./models/*"],
        "pages/*":["./pages/*"],
        "services/*":["./services/*"],
        "styledComponents/*":["./styledComponents/*"],
        "utilities/*":["./utilities/*"]
      }
    }
}