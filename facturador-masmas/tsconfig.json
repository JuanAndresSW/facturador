{
  "include": [
    "src/*",
    "declaration.d.ts"
],
  "exclude": [
    "node_modules",
    "build"
  ],
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "ES2020",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./src/",
      "paths": {
        "assets/*":["./assets/*"],
        "components/*":["./components/*"],
        "pages/*":["./pages/*"],
        "services/*":["./services/*"],
        "styles/*":["./styles/*"],
        "utils/*":["./utils/*"]
      }
    }
}